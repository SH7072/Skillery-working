                    <label for="link">Add Lecture Link</label><br>
                    <input type="text" id="link" placeholder="Add Lecture Link" name="link" required>
                    <label for="link">Add Homework</label><br>
                    <input type="text" id="homework" placeholder="Add Homework" name="homework" required>
                    <button type="submit" class="btn cancel" id="submit">Submit</button>
                  </form>
