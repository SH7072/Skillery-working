              <% records.forEach(function(row){ %>
                <div class="row rowl">
                    <div class="col-3 overflow-auto">
                        <%= row.topicName %>
                    </div>
                    <a class="col-5 overflow-auto">
                    <%= row.lectureLink %>
                    </a>
                    <a class="col-2 overflow-auto">
                    <%= row.homeworkLink %>
                    </a>
                </div>
            <% })%>
              
               
              <button id="addday">Add day</button>
              <div class="loginPopup">
                <div class="formPopup" id="popupForm">
