    <script>
    $('.owl-carousel').owlCarousel({
        loop:true,
        nav:true,
        margin:10,
        navSlide:true,
        dots:false,
        stagePadding:0,
        responsive:{
            0:{
                items:1
            },
            500:{
                items:3
            },
            800:{
                items:5
            },
            1000:{
                items:7
            }
        }
    })</script>
